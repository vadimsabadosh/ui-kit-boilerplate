{
  "name": "ui-kit-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for React UI Kit",
  "author": "Vadym Sabadosh <vadym.sabadosh@anvileight.com>",
  "license": "ISC",
  "main": "./dist/index.js",
  "private": false,
  "dependencies": {
    "classnames": "^2.2.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build:css": "node-sass src/ -o src/ --output-style compressed",
    "build": "tsc --build \"./tsconfig.json\"",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --max-warnings=0",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "deploy-storybook:ghpages": "storybook-to-ghpages",
    "pub": "npx webpack && npm version patch && npm publish"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-links": "^6.5.14",
    "@storybook/builder-webpack5": "^6.5.14",
    "@storybook/manager-webpack5": "^6.5.14",
    "@storybook/node-logger": "^6.5.14",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.5.14",
    "@storybook/storybook-deployer": "^2.8.16",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/node-sass": "^4.11.1",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.11.1",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.1",
    "eslint": "^7.17.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-testing-library": "^3.10.1",
    "node-sass": "^5.0.0",
    "prettier": "^2.8.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "^5.0.1",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "classnames": "*"
  }
}
